<h1><%= t('.header') %></h1>
<p><%= link_to t('.new_test'), new_admin_test_path %></p>
<% @tests_pages.each do |test| %>
  <table>
    <tr>
      <td><%= link_to test.title, admin_test_path(test) %></td>
      <td><%= button_to t('.delete'), admin_test_path(test),
                                  method: :delete,
                                  data: { confirm: 'Are you sure?' } %></td>
    </tr>
  </table>
<% end %>
<p><%= paginate @tests_pages %></p>
