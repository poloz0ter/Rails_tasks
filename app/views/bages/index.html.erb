<% @bages.each do |bage| %>
  <nav class="mt-2">
    <% if @user_bages.include?(bage) %>
      <img src="<%= bage.image_url %>" style="width:100px;height:100px;">
    <% else %>
      <img src="https://cdn.pixabay.com/photo/2017/02/12/21/29/false-2061132_960_720.png" style="width:100px;height:100px;">
    <% end %>
      <b><%= bage.name %></b>
  </nav>
<% end  %>
